{
  "name": "GOD",
  "version": "0.1.0",
  "description": "Autonomous development powered by Agentic OS",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx src/index.ts",
    "api": "tsx src/platform/api/server.ts",
    "build": "tsc",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "cdk": "cdk",
    "cdk:deploy": "cdk deploy --all",
    "cdk:destroy": "cdk destroy --all",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "deploy:staging": "bash scripts/deploy.sh staging",
    "deploy:production": "bash scripts/deploy.sh production",
    "rollback:staging": "bash scripts/rollback.sh staging",
    "rollback:production": "bash scripts/rollback.sh production",
    "docker:build": "docker build -t god-agent-runtime .",
    "docker:run": "docker run -p 3000:3000 god-agent-runtime"
  },
  "keywords": [
    "agentic-os",
    "autonomous"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^3.2.4",
    "aws-cdk-lib": "^2.230.0",
    "constructs": "^10.4.3",
    "eslint": "^8.54.0",
    "source-map-support": "^0.5.21",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.940.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.940.0",
    "@aws-sdk/client-dynamodb": "^3.940.0",
    "@aws-sdk/client-opensearch": "^3.940.0",
    "@aws-sdk/client-sns": "^3.940.0",
    "@aws-sdk/client-sqs": "^3.940.0",
    "@aws-sdk/lib-dynamodb": "^3.940.0",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "aws-xray-sdk-core": "^3.12.0",
    "uuid": "^13.0.0"
  }
}
